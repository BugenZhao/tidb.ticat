help = load workload data to a given empty cluster then do physical backup
abbr = bench.pp

flow = \
	tidb.destroy : tidb.new : [[bench.workload]].load : tidb.stop : \
	[[bench.workload]].gen-tag : tidb.backup.gen-tag : tidb.backup.backup

[env]
bench.workload = read
tidb.backup.tag-from = workload-name,workload-tag
